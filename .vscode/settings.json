{
    "actionButtons": {
        "defaultColor": "#FFFFFF",
        "commands": [
            {
                "name": "Activate venv",
                "command": "source $HOME/micropython/venv/bin/activate"
            },
            {
                "name": "make clean (Unix)",
                "command": "cd $HOME/micropython/ports/unix; make clean"
            },
            {
                "name": "make with ep_modules (Unix)",
                "command": "cd $HOME/micropython/ports/unix; make USER_C_MODULES=$HOME/micropython/ep_modules CFLAGS_EXTRA=-DMODULE_EPMODULES_ENABLED=1 all"
            },
            {
                "name": "make with ep_modules (ESP32)",
                "command": "cd $HOME/micropython/ports/esp32; make PYTHON2=$HOME/micropython/venv/bin/python3 USER_C_MODULES=$HOME/micropython/ep_modules CFLAGS_EXTRA=-DMODULE_EPMODULES_ENABLED=1 all"
            }
        ]
    },
